[user]
	name = David Mailhot
	email = djmailhot@gmail.com
[core]
	autocrlf = input
	safecrlf = true
	safecrf = true
	editor = vim
	excludesfile = .gitignore_global
[alias]
  graph = log --graph --full-history --all --color --pretty=tformat:"%x1b[31m%h%x09%x1b[32m%d%x1b[0m%x20%s%x20%x1b[33m(%an)%x1b[0m"
